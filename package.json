{
  "name": "@codemonger-io/cdk-cors-utils",
  "version": "0.4.1",
  "description": "CORS utilities for CDK v2",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "doc": "run-s build && api-extractor run --local && api-documenter markdown --input-folder temp --output-folder api-docs/markdown",
    "type-check": "tsc --noEmit",
    "prepare": "run-s build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cdk",
    "CORS"
  ],
  "author": "Kikuo Emoto <kemoto@codemonger.io>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.28.2",
    "@microsoft/api-extractor": "^7.55.2",
    "aws-cdk-lib": "^2.234.1",
    "constructs": "10.4.4",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.1.2",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "aws-cdk-lib": ">=2.0.0"
  }
}
